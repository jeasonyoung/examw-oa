<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.examw.oa.domain.organization">
	<!-- 员工注册共用User -->
	<!--员工表-->
	<class name="Empl"  table="tbl_oa_dept_employee">
	    <!-- 员工ID -->
	    <id name="id" type="string"  length="64"  />
	    <!-- 员工编号 -->
	    <property name="code" type="string"  not-null="true"  />
	    <!-- 员工 姓名-->
	    <property name="name" type="string"  not-null="true"  />
	    <!-- 员工性别-->
	    <property name="sex" type="string"  length="255"  />
	    <!-- 员工手机号码 -->
		<property name="phone" column="create_time" type="integer"/>
		<!-- 员工邮箱地址 -->
		<property name="email" column="last_time" type="string"/>
		<!-- 员工身份证号码-->
		<property name="idcard" column="create_username" type="string"  />
		<!-- 创建时间 -->
		<property name="create_time" column="create_time" type="timestamp"  />
		<!-- 密码 -->
		<property name="password"  type="string"/>
		<!-- 出生日期-->
		<property name="both" column="both_time" type="timestamp"  />
		<!-- 入职时间 -->
		<property name="entry_time" column="entry_time" type="timestamp"/>
		<!-- 部门 -->
		<many-to-one name="dept"  column="dept_id"  class="" />
		<!-- 职务 -->
		<many-to-one name="post"  column="post_id"  class="" />
		
	</class>
	<!--部门实体-->
	<class name="Dept"  table="tbl_oa_org_department">
		<!--部门ID -->
		<id name="id"  type="string"  length="64" />
		<!-- 部门名称 -->
		<property name="name"  type="string"  length="50"  not-null="true"/>
		<!-- 部门编号-->
		<property name="deptno"  type="string" />
		<!-- 负责人 -->
		<property name="deptno" type="string" />
		<!-- 成立时间-->
		<property name="found_time" type="timestamp" column="found_time"/>
		<!--计划 -->
		<many-to-one name="plan"  column="plan_id"  cascade="none"  class=""/>
		
	</class>
	<!-- 职位数据 -->
	<class name="PaperStructure"  table="tbl_Netplatform_Exam_PaperStructure">
		<!--职位ID -->
		<id name="id"  type="string"  length="64" />
		<!-- 职位名称 -->
		<property name="name"  type="string"   not-null="true"/>
		<!-- 职位编号 -->
		<property name="jobcode"  type="string"   not-null="true"/>
	</class>
	<!-- 职务数据 -->
	<class name="Note"  table="tbl_Netplatform_Exam_Note">
		<!--职务ID -->
		<id name="id"  type="string"  length="64" />
		<!-- 职务名称 -->
		<property name="note"  type="text" not-null="true"/>
		<!-- 职务等级 -->
		<property name="createTime" column="create_time" type="timestamp"/>
		
	</class>
</hibernate-mapping>